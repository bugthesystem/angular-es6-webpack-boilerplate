{
  "name": "ng-espack-boilerplate",
  "version": "1.0.0",
  "description": "Angular 1.4x + ES6 boilerplate application using Webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.build.js --bail -p",
    "test": "karma start",
    "test:live": "karma start --auto-watch --no-single-run",
    "preupdate-webdriver": "npm install",
    "update-webdriver": "webdriver-manager update",
    "preprotractor": "npm run update-webdriver",
    "protractor": "protractor e2e-tests/protractor.conf.js",
    "eslint:app": "eslint app/**",
    "eslint:tests": "eslint tests/**",
    "test-travis": "karma start && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ziyasal/ng-espack-boilerplate.git"
  },
  "keywords": [
    "angular",
    "webpack",
    "es6",
    "babel",
    "isparta",
    "boilerplate",
    "eslint",
    "jasmine",
    "karma"
  ],
  "author": "ziyasal",
  "license": "MIT",
  "devDependencies": {
    "angular-mocks": "1.4.8",
    "autoprefixer": "6.0.3",
    "babel-core": "6.2.1",
    "babel-loader": "6.2.0",
    "babel-preset-es2015": "6.1.18",
    "chai": "3.4.1",
    "css-loader": "0.23.0",
    "eslint": "1.10.3",
    "eslint-config-defaults": "7.1.1",
    "extract-text-webpack-plugin": "0.9.1",
    "file-loader": "0.8.4",
    "gulp": "3.9.0",
    "gulp-angular-templatecache": "1.8.0",
    "gulp-concat": "2.6.0",
    "gulp-livereload": "3.8.1",
    "gulp-sourcemaps": "1.6.0",
    "gulp-uglify": "1.5.1",
    "gulp-usemin": "0.3.16",
    "gulp-util": "3.0.7",
    "html-webpack-plugin": "1.6.2",
    "istanbul-instrumenter-loader": "1.0.0",
    "jasmine-core": "2.4.1",
    "json-server": "0.8.4",
    "karma": "0.13.15",
    "karma-chrome-launcher": "0.2.2",
    "karma-coverage": "0.5.3",
    "karma-jasmine": "0.3.6",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-sourcemap-loader": "0.3.6",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "1.7.0",
    "node-libs-browser": "0.5.3",
    "null-loader": "0.1.1",
    "phantomjs": "1.9.18",
    "postcss-loader": "0.8.0",
    "raw-loader": "0.5.1",
    "run-sequence": "1.1.5",
    "style-loader": "0.13.0",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.12.1"
  },
  "dependencies": {
    "angular": "1.5.9",
    "angular-animate": "1.4.8",
    "angular-ui-router": "0.2.18",
    "angularjs-toaster": "0.4.15",
    "bootstrap": "3.3.7",
    "jquery": "2.2.4",
    "lodash": "4.13.1",
    "phantomjs": "1.9.20"
  }
}
